{
    "name": "decrown-workers-transportation",
    "version": "1.0.0",
    "description": "Worker transportation management system with real-time tracking and automated billing",
    "main": "dist/index.js",
    "type": "commonjs",
    "scripts": {
        "build": "tsc",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "start": "node dist/index.js",
        "test": "vitest --run",
        "test:watch": "vitest",
        "test:unit": "vitest --run src/test/services src/test/middleware src/test/routes",
        "test:e2e": "vitest --run src/test/e2e",
        "test:e2e:journey": "vitest --run src/test/e2e/workerTransportationJourney.test.ts",
        "test:e2e:payment": "vitest --run src/test/e2e/paymentProcessingBilling.test.ts",
        "test:e2e:emergency": "vitest --run src/test/e2e/emergencyScenarios.test.ts",
        "test:e2e:concurrent": "vitest --run src/test/e2e/multiUserConcurrency.test.ts",
        "migrate": "knex migrate:latest",
        "migrate:rollback": "knex migrate:rollback",
        "seed": "knex seed:run",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
    },
    "keywords": [
        "transportation",
        "worker",
        "tracking",
        "billing",
        "payment",
        "logistics"
    ],
    "author": "DeCrown",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-kms": "^3.450.0",
        "@types/ioredis": "^4.28.10",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.21.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "ioredis": "^5.8.2",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.0.1",
        "node-cron": "^3.0.3",
        "pg": "^8.11.3",
        "redis": "^4.6.10",
        "toml": "^3.0.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.10.0",
        "@types/node-cron": "^3.0.8",
        "@types/pg": "^8.10.9",
        "@types/supertest": "^2.0.16",
        "@types/uuid": "^9.0.7",
        "@types/ws": "^8.5.8",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "eslint": "^8.55.0",
        "supertest": "^6.3.3",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.2"
    },
    "directories": {
        "doc": "docs"
    }
}
